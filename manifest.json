{
  "manifest_version": 3,
  "name": "Buzzlytics Extension",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "permissions": [],
  "host_permissions": ["https://*/*"],
  "content_scripts": [
	{
		"js": ["src/content.tsx"],
		"matches": ["https://*.tiktok.com/*"],
		"run_at": "document_end"
	}
  ]
}
